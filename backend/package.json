{
  "name": "autotoken-backend",
  "version": "1.0.0",
  "description": "AutoToken RWA Platform Backend API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js"
  },
  "keywords": [
    "blockchain",
    "rwa",
    "nft",
    "automotive",
    "ethereum",
    "gasless"
  ],
  "author": "AutoToken Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@biconomy/mexa": "^3.0.6",
    "@openzeppelin/contracts": "^5.4.0",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ethers": "^5.8.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "redis": "^5.9.0",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "web3": "^4.16.0"
  },
  "devDependencies": {
    "eslint": "^9.38.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.4"
  }
}
